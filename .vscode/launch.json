        {
            "type": "java",
            "name": "Launch MudamuApplication",
            "request": "launch",
            "mainClass": "com.Mudamu.MudamuApplication",
            "projectName": "muDamu",
            "vmArgs": "--add-opens java.base/java.lang=ALL-UNNAMED"
        },
        {
            "type": "java",
            "name": "Launch Passgenerator",
            "request": "launch",
            "mainClass": "com.Mudamu.util.Passgenerator",
            "projectName": "muDamu"
        }
    ]
